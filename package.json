{
  "name": "odd-web3storage",
  "version": "0.1.0",
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json",
    "./*": [
      "./lib/*.js",
      "./lib/*",
      "./lib/*/index.js",
      "./*"
    ]
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "lib/index.d.ts": [
        "lib/index.d.ts"
      ],
      "*": [
        "lib/*"
      ]
    }
  },
  "files": [
    "lib",
    "dist",
    "docs",
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "!*.test.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/icidasset/odd-web3storage"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@ipld/dag-ucan": "^3.4.0",
    "@web3-storage/access": "^15.2.1",
    "@web3-storage/capabilities": "^9.3.0",
    "@web3-storage/did-mailto": "^2.0.0",
    "@web3-storage/upload-client": "^9.2.0",
    "@web3-storage/w3up-client": "^8.0.3",
    "blockstore-level": "^1.1.4",
    "uint8arrays": "^4.0.6"
  },
  "peerDependencies": {
    "@oddjs/odd": "icidasset/ts-odd#authority"
  },
  "devDependencies": {
    "typescript": "^5.2.2"
  }
}
